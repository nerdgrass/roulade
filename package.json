{
  "name": "roulade",
  "version": "0.0.4",
  "description": "React and Redux bindings for GraphQL",
  "bin": {
    "babel-webpack-package-boilerplate": "./bin/main.js"
  },
  "main": "dist/index.js",
  "files": [
    "bin",
    "build",
    "src",
    "test",
    ".babelrc",
    "webpack.config.js"
  ],
  "directories": {
    "lib": "src/lib"
  },
  "scripts": {
    "build": "rimraf build && webpack",
    "prestart": "npm run build",
    "start": "node bin/main.js",
    "pretest": "npm run build",
    "test": "mocha build/test.js  --require babel-core/register",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nerdgrass/roulade.git"
  },
  "keywords": [
    "react",
    "es2015",
    "redux",
    "graphQL",
    "relay"
  ],
  "author": "Alex Boerstler <aboerstler@paintnite.com>",
  "contributors": [
    {
      "name": "Chris Chambers",
      "email": "cchambers@paintnite.com"
    },
    {
      "name": "Jordan Vishniak",
      "email": "jvishniac@paintnite.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nerdgrass/roulade/issues"
  },
  "homepage": "https://github.com/nerdgrass/roulade",
  "dependencies": {
    "lodash": "^4.13.1",
    "lokka": "^1.7.0",
    "lokka-transport-http": "^1.4.0",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.1",
    "webpack": "^1.12.12"
  }

}
